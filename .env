# Environment
VITE_NODE_ENV=development

# Firebase Configuration - Development
VITE_FIREBASE_DEV_API_KEY=mock-api-key-for-development
VITE_FIREBASE_DEV_AUTH_DOMAIN=lawnsync-app-dev.firebaseapp.com
VITE_FIREBASE_DEV_PROJECT_ID=lawnsync-app-dev
VITE_FIREBASE_DEV_STORAGE_BUCKET=lawnsync-app-dev.appspot.com
VITE_FIREBASE_DEV_MESSAGING_SENDER_ID=mock-sender-id
VITE_FIREBASE_DEV_APP_ID=mock-app-id
VITE_FIREBASE_DEV_MEASUREMENT_ID=mock-measurement-id
VITE_FIREBASE_DEV_VAPID_KEY=mock-vapid-key

# Feature Flags - Enable mock mode
VITE_USE_MOCK_FIREBASE=true
VITE_USE_MOCK_WEATHER=true
VITE_USE_MOCK_OPENAI=true

# Emulator Configuration
VITE_USE_EMULATORS=false